% Trace / Spur
\DeclareMathOperator{\Tr}{Tr}
\DeclareMathOperator{\Sp}{Sp}

%-- Norm
\newcommand{\norm}[2]{\left\lVert#1\right\rVert_{\ensuremath{#2}}}

%-- Ableitungen
\newcommand{\partialfrac}[2] {\frac{\partial #1}{\partial #2}}
\newcommand{\difffrac}[2]    {\frac{\text{d} #1}{\text{d} #2}}
%-- Partielle Ableitungen
%-- Aufruf �ber \dpart{}{} bzw- \tdpart{}{}
\newcommand{\dpart}[2]{\ensuremath{\frac{\partial {#1}}{\partial {#2}}}}  
\newcommand{\tdpart}[2]{\ensuremath{\tfrac{\partial {#1}}{\partial {#2}}}}  

%-- Sinus und Cosinus
\newcommand{\tcos}[1]{\text{c}_{#1}}
\newcommand{\tsin}[1]{\text{s}_{#1}} 

%--  Matrizen (Fettdruck)
%\newcommand{\vmatrix}[1]{\ensuremath{\mathbf{#1}}}

%-- Vektoren (Klein + Fettdruck)
\renewcommand {\vec}[1]{\ensuremath{\mathbf{#1}}}
\newcommand {\vdot}[1]{\ensuremath{\dot{\mathbf{#1}}}}
\newcommand {\vddot}[1]{\ensuremath{\ddot{\mathbf{#1}}}}
\newcommand {\vhat}[1]{\ensuremath{\hat{\mathbf{#1}}}}
\newcommand {\vtilde}[1]{\ensuremath{\tilde{\mathbf{#1}}}}
\newcommand {\vcheck}[1]{\ensuremath{\check{\mathbf{#1}}}}
\newcommand {\vbar}[1]{\ensuremath{\bar{\mathbf{#1}}}}

%-- Vektoren (zeitabh�ngig (t)) (Klein + Fettdruck)
\newcommand {\tvec}[1]{\ensuremath{\mathbf{#1}(t)}}
\newcommand {\dottvec}[1]{\ensuremath{\dot{\mathbf{#1}(t)}}}
\newcommand {\ddottvec}[1]{\ensuremath{\ddot{\mathbf{#1}(t)}}}

%-- Vektoren von Symbolen (Klein + Fettdruck)
\newcommand {\symvec}[1]{\ensuremath{\boldsymbol{#1}}}
\newcommand {\dotsymvec}[1]{\ensuremath{\dot{\boldsymbol{#1}}}}
\newcommand {\ddotsymvec}[1]{\ensuremath{\ddot{\boldsymbol{#1}}}}

%-- Funktionen von Vektoren (Fettdruck)
\newcommand {\func }[1]{\ensuremath{\mathbf{#1}}}
\newcommand {\symfunc}[1]{\ensuremath{\boldsymbol{#1}}}

%-- R�mische Schrift (Gro�)
\newcommand{\RM}[1]{\Romannum{#1}}

%-- Namen
\newcommand{\Rang}{\mathrm{Rang}}
%\newcommand{\det}{\mathrm{det}}

%-- Number sets
\newcommand{\Rm}{\mathbb{R}}

%-- Schreibweise Exponenten
\newcommand{\vExp}[3] {\prescript{\mathrm{#1}}{}{\boldsymbol{#2}}_{\mathrm{#3}}} %{\leftidx{_{}^#1} {\boldsymbol{#2}} {_#3} }  


